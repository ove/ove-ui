{
  "name": "@ove/ove-ui-launcher",
  "version": "0.2.0",
  "description": "Launch applications on the OVE framework",
  "main": "server.js",
  "private": true,
  "keywords": [],
  "author": "Senaka Fernando <senaka.fernando15@imperial.ac.uk>",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "5.0.1",
    "codemirror": "5.61.1",
    "axios": "0.21.1",
    "cors": "2.8.5",
    "dotenv": "8.6.0",
    "express": "4.17.1",
    "jquery": "3.5.0",
    "js-beautify": "1.10.3",
    "little-loader": "0.2.0",
    "react-codemirror2": "7.2.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.4",
    "react-scripts": "3.4.4",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "0.88.2"
  },
  "scripts": {
    "start": "PORT=8281 node -r dotenv/config server.js dotenv_config_path=.env",
    "start:dev": "OVE_HOST='localhost:8080' PORT=8281 node -r dotenv/config ../../node_modules/.bin/react-scripts start dotenv_config_path=.env",
    "start:windows": ".\\\"node_modules/.bin/react-scripts\" start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "lint": "eslint src/. --ext .jsx"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
